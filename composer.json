{
    "name": "queencitycodefactory/cakespreadsheet",
    "description": "CakePHP 3 PHP Spreadsheet",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "php", "excel", "spreadsheet"],
    "homepage": "https://github.com/QueenCityCodeFactory/CakeSpreadsheet.git",
    "license": "MIT",
    "authors":[
        {
            "name":"darensipes",
            "role":"Author",
            "homepage":"https://github.com/darensipes"
        }
    ],
    "support":{
        "source":"https://github.com/QueenCityCodeFactory/CakeSpreadsheet",
        "issues":"https://github.com/QueenCityCodeFactory/CakeSpreadsheet/issues"
    },
    "require": {
        "php": ">=7.2.0",
        "cakephp/cakephp": "^3.5.10",
        "phpoffice/phpspreadsheet": "^1.6.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeSpreadsheet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
           "CakeSpreadsheet\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
